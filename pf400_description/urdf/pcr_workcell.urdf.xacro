  <joint name="OT2" type="fixed">
    <parent link="module_cart_link_2"/>
    <child link="ot2/OT2"/>
    <axis xyz="0 0 1"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <limit effort="1000.0" lower="0" upper="0" velocity="0.5"/>
