<?xml version="1.0"?>
<robot name="PCR Workcell" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Materials -->

  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>

  <material name="black">
    <color rgba="0.2 0.2 0.2 1"/>
  </material>

  <material name="blue">
    <color rgba="0 0 0.8 1"/>
  </material>

  <material name="red">
    <color rgba="0.8 0.2 0.2 1"/>
  </material>

  <material name="pink">
    <color rgba="0.6 0.4 0.6 1"/>
  </material>

  <material name="dark_grey">
    <color rgba="0.4 0.4 0.4 1"/>
  </material>

  <material name="light_grey">
    <color rgba="0.9 0.9 0.9 1"/>
  </material>

  <material name="dark_blue">
    <color rgba="0.011764705882352941 0.027450980392156862 0.29215686274509803 1.0"/>
  </material>

  <material name="light_blue">
    <color rgba="0 0.5 0.8 1.0"/>
  </material>

  <material name="darkbluegrey" >
    <color rgba="0.12156862745098039 0.23137254901960785 0.30196078431372547 1.0" />
  </material>
  
  <material name="darkbluegreen" >
    <color rgba="0.0 0.1 0.13 1.0" />
  </material>

  <material name="aluminum">
    <color rgba="0.6 0.6 0.6 1"/>
  </material>
  
  <!-- PF400 -->
  <xacro:include filename="$(find pf400_description)/urdf/PF400.urdf.xacro" />
  <xacro:pf400 prefix="pf400/"/>

  <!-- Peeler -->
  <xacro:include filename="$(find pf400_description)/urdf/peeler.urdf.xacro" />
  <xacro:Peeler prefix="peeler/"/>

  <!-- Sealer -->
  <xacro:include filename="$(find pf400_description)/urdf/sealer.urdf.xacro" />
  <xacro:Sealer prefix="sealer/"/>
  
  <!-- OT2 -->
  <xacro:include filename="$(find pf400_description)/urdf/OT2.urdf.xacro" />
  <xacro:OT2 prefix="ot2/"/>

  <!-- Thermocycler -->
  <xacro:include filename="$(find pf400_description)/urdf/thermocycler.urdf.xacro" />
  <xacro:Thermocycler prefix="thermocycler/"/>
  
  <!-- Links -->
  

  <link name="OT2_2">
    <visual>
      <origin xyz="0.975 -1.02 1.042" rpy="1.5708 0 3.1416"/>
      <geometry>
        <mesh filename="package://pf400_description/meshes/OT_2_without_pipette.STL" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="aluminum"/>
    </visual>
  </link>

  <link name="module_cart_link_1">
    <visual>
      <origin xyz="1.32 0.847 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://pf400_description/meshes/module_cart.stl" scale="1 1 1"/>
      </geometry>
      <material name="darkbluegreen"/>
    </visual>
  </link>

  <link name="module_cart_link_2">
    <visual>
